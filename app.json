{
  "expo": {
    "name": "شهادتي",
    "slug": "chehadeti-app",
    "version": "0.0.38",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/icons/icon.png",
    "scheme": "chehadeti-app",
    "userInterfaceStyle": "automatic",
    "updates": {
      "url": "https://u.expo.dev/4abaa9ee-c3c1-40bb-930b-ddad5625239f"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "splash": {
      "image": "./assets/images/logo-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "appleTeamId": "J25B6JQ586",
      "infoPlist": {
        "NSCameraUsageDescription": "Chehadeti uses the camera to take and upload student photos during enrollment and when updating school profiles",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.chehadeti.admin",
      "buildNumber": "0.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "VIBRATE",
        "SYSTEM_ALERT_WINDOW"
      ],
      "package": "com.chehadeti.admin",
      "versionCode": 14
    },
    "web": {
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-updates",
      "expo-secure-store",
      "expo-router",
      "expo-asset",
      "expo-font",
      "expo-web-browser",
      "expo-localization",
      "./android-material.plugins",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "newArchEnabled": true
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1",
            "newArchEnabled": true
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4abaa9ee-c3c1-40bb-930b-ddad5625239f"
      }
    }
  }
}
